package com.cozen;

public class collection1 {
	
	
	private int id;
	private String name;
	private String Password;
	
	
	public collection1(){
		
	}		
	public collection1(int id, String name, String password) {
		super();
		this.id = id;
		this.name = name;
		Password = password;
	}
	public int getId() {
		return id;
	}

    public void setId(int id) {
		this.id = id;
	}
    public String getName() {
		return name;
	}

    public void setName(String name) {
		this.name = name;
	}
   public String getPassword() {
		return Password;
	}
   public void setPassword(String password) {
		Password = password;
	}

	@Override
	public String toString() {
		return " [id=" + id + ", name=" + name + ", Password=" + Password + "]";
	}

	
}
